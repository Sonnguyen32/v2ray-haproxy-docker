          "path": "/discussion/",
          "headers": {
            "Host": "ninisite.com"
          }
        }
      }
    },
    {
      "tag": "shadowsocks-ws",
      "port": 82,
      "protocol": "shadowsocks",
      "settings": {
        "method": "aes-128-gcm",
        "ota": true,
        "password": "<SHADOWSOCKS_PASSWORD>"
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "path": "/topics/sounds/mazhabi/",
          "headers": {
            "Host": "download.ir"
          }
        }
      }
    },
    {
      "tag": "vless-ws",
      "port": 83,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "<UUID>",
            "level": 0
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "connectionReuse": true,
          "path": "/Play/",
          "headers": {
            "Host": "sound.tebyan.net"
          }
        }
      }
