  "inbounds": [
    {
      "tag": "vmess-ws",
      "port": 81,
      "protocol": "vmess",
      "settings": {
        "clients": [
          {
            "id": "<UUID>",
            "alterId": 65
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
