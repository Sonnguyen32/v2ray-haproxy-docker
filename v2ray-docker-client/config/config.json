    {
      "tag": "vmess-ws",
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "<BRIDGE-DOMAIN-OR-IP>",
            "port": 81,
            "users": [
              {
                "id": "<UUID>",
                "alterId": 65
