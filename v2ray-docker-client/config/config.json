        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "connectionReuse": true,
          "path": "/discussion/",
          "headers": {
            "Host": "ninisite.com"
          }
        }
      },
      "mux": {
        "enabled": true
      }
    },
    {
      "tag": "shadowsocks-ws",
      "protocol": "shadowsocks",
      "settings": {
        "servers": [
          {
            "address": "<BRIDGE-DOMAIN-OR-IP>",
            "port": 82,
            "method": "aes-128-gcm",
            "password": "<SHADOWSOCKS_PASSWORD>"
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "connectionReuse": true,
          "path": "/topics/sounds/mazhabi/",
          "headers": {
            "Host": "download.ir"
          }
        }
      },
      "mux": {
        "enabled": true
      }
    },
    {
      "tag": "vless-ws",
      "protocol": "vless",
      "settings": {
        "vnext": [
          {
            "address": "<BRIDGE-DOMAIN-OR-IP>",
            "port": 83,
            "users": [
              {
                "id": "<UUID>",
                "encryption": "none",
                "level": 0
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "ws",
        "wsSettings": {
          "connectionReuse": true,
          "path": "/Play/",
          "headers": {
            "Host": "sound.tebyan.net"
          }
        }
      },
      "mux": {
        "enabled": true
      }
    },
